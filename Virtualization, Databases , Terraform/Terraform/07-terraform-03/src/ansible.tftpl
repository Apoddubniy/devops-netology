[webservers]

%{~ for i in web ~}

${i["name"]}   ansible_host=${i["network_interface"][0]["nat_ip_address"]}
%{~ endfor ~}


[databases]

%{~ for k in databases ~}

${k["name"]}   ansible_host=${k["network_interface"][0]["nat_ip_address"]}
%{~ endfor ~}


[storage]

%{~ for s in storage ~}

${s["name"]}   ansible_host=${s["network_interface"][0]["nat_ip_address"]}
%{~ endfor ~}